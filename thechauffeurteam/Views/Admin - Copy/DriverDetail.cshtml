                                                        <img src="data:image;base64,@Convert.ToBase64String(Model.DriverImage)" style="width:200px" data-toggle="modal" data-target="#driverImgModel" />
                                                    }

                                                    @*<input type="file"><img src="data:image;base64,@Convert.ToBase64String(Model.DriverImage)" style="margin:-2PX" />*@
                                                </div>
                                            </div>

