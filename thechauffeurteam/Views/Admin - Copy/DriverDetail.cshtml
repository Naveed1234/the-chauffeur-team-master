                                                            <div class="col-sm-12">
                                                                <input type="hidden" id="dob" value="@Model.DateOfBirth" class="form-control" />
                                                                @Html.EditorFor(model => model.DateOfBirth, new { htmlAttributes = new { @class = "form-control", id = "dateOB" } })
                                                                @Html.ValidationMessageFor(model => model.DateOfBirth, "", new { @class = "text-danger" })

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
